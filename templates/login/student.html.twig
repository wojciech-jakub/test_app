{% extends 'base.html.twig' %} 
{% block javascripts %} 
   <script language = "javascript" src = "https://code.jquery.com/jquery-2.2.4.min.js"></script> 
     <script language = "javascript">  
      $(document).ready(function(){   
         $("#fileToUpload").on("click", function(event){  
            $.ajax({  
               url:        "/student/new",  
               method:       'POST',   
               dataType:   'json',  
               
               success: function(data, status) {  
                   
                    alert('Ajaasdasdasd');  
               },  
            });  
         });  
      });  
   </script> 
{% endblock %} 
{% block stylesheets %} 
   <style> 
      #simpleform { 
         width:600px; 
         border:2px solid grey; 
         padding:14px; 
      }  
      #simpleform label { 
         font-size:12px; 
         float:left; 
         width:300px; 
         text-align:right; 
         display:block; 
      } 
      #simpleform span { 
         font-size:11px; 
         color:grey; 
         width:100px; 
         text-align:right; 
         display:block; 
      }  
      #simpleform input { 
         border:1px solid grey; 
         font-family:verdana; 
         font-size:14px; 
         color:grey; 
         height:24px; 
         width:250px; 
         margin: 0 0 20px 10px; 
      }  
      #simpleform button { 
         clear:both; 
         margin-left:250px; 
         background:grey; 
         color:#FFFFFF; 
         border:solid 1px #666666; 
         font-size:16px; 
      } 
   </style> 
{% endblock %} 
{% block body %} 
   <h3>Student form</h3> 
   <div id="simpleform"> 
      {{ form_start(form) }} 
      {{ form_widget(form) }} 
      {{ form_end(form) }} 
    <form action="/student/new" method="POST">
        <input type="hidden" value="1" name="upload">
        <input type="file" name="myFile" accept="image/*">
    
        <input type="submit" id="fileToUpload">
    </form>

   </div>   
{% endblock %} 